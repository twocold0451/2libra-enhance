{
  "manifest_version": 3,
  "name": "2libra-enhance",
  "version": "1.1.0",
  "description": "2libra.com 论坛增强插件：帖子快速预览",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://2libra.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ],
  "host_permissions": ["https://2libra.com/*"],
  "action": {
    "default_title": "2libra-enhance"
  }
}